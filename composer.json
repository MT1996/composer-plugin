{
    "name": "theworldscms/composer-plugin",
    "description": "Custom Composer Class for Installation of Packages",
    "type": "composer-plugin",
    "require": {
        "composer-plugin-api": "^1.1"
    },
    "require-dev": {
        "composer/composer": "^1.10.6"
    },
    "config": {
        "optimize-autoloader": true
    },
    "autoload": {
        "psr-4": {
            "TheWorldsCMS\\": "./Classes/"
        }
    },
    "extra": {
        "class": "TheWorldsCMS\\Composer\\ComposerInstallerPlugin"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Marc Totzke",
            "email": "Marc.Totzke@t-online.de"
        }
    ]
}
