{
    "name": "theworldscms/composer-plugin",
    "description": "Custom Composer Class for Installation of Packages",
    "type": "composer-plugin",
    "require": {
        "composer-plugin-api": "^1.0.0"
    },
    "require-dev": {
        "composer/composer": "^1.10.0"
    },
    "conflict": {
        "composer/installers": "*"
    },
    "autoload": {
        "psr-4": {
            "TheWorldsCMS\\Composer\\": "Classes/"
        }
    },
    "extra": {
        "class": "TheWorldsCMS\\Composer\\ComposerInstallerPlugin"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Marc Totzke",
            "email": "Marc.Totzke@t-online.de"
        }
    ]
}
